{
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "?",
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "proto3",
    "py",
    "python",
	"typescript",
	"typescriptreact"
  ],
  "typescript.suggest.autoImports": true,
  "typescript.referencesCodeLens.enable": true,
  "html.format.enable": true,
  "tsserver.formatOnType": true,
  "workspace.workspaceFolderCheckCwd": false,
  "tsserver.tsdk": ".yarn/bin/typescript/lib",
  "typescript.format.enabled": true,
  "typescript.format.semicolons": "insert",
  "tsserver.disableAutomaticTypeAcquisition": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/bin",
  "eslint.format.enable": true,
  "eslint.autoFixOnSave": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
